{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "interface-name": [false],
        "object-literal-sort-keys": false,
        "no-implicit-dependencies": [true, "dev"],
        "quotemark": [true, "single"],
        "trailing-comma": [
            true,
            {
                "esSpecCompliant": true
            }
        ],
        "member-ordering": false,
        "variable-name": false,
        "no-empty": false,
        "no-console": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": []
    }
}
